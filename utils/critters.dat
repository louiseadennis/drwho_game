DROP TABLE IF EXISTS `critters`;
CREATE TABLE `critters` (
  `critter_id` int(11) NOT NULL AUTO_INCREMENT,
  `name` text,
  `icon` text,
  `stories` text,
  PRIMARY KEY (`critter_id`)
) ENGINE=InnoDB AUTO_INCREMENT=33 DEFAULT CHARSET=latin1;

